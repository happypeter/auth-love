<!DOCTYPE html>
<html>
<head>
  <title>AuthLove</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<% flash.each do |name, msg| %>  
   <%= content_tag :div, msg, :id => "flash#{name}" %>  
<% end %>  
   

<%= yield %>

</body>
</html>
